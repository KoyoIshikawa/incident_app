<h1>検索結果</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Height</th>
      <th>Weight</th>
      <th>Gender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |incident| %>
      <tr>
        <td><%= incident.incident %></td>
        <td><%= incident.solution %></td>
        <td><%= incident.os_name.name %></td>
        <td><%= incident.status.status %></td>
        <td><%= incident.coding_lang.name %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Top', incidents_path %>